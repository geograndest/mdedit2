                "de": "config/views/home/locales.de.json"
            }
        },
        "mdEditData": {
            "url": "app.mdEditData",
            "path": "config/views/md-edit-data/md-edit-data.html",
            "icon": "glyphicon-eye-open",
            "type": "mdEdit",
            "default": true,
            "hierarchyLevels": ["dataseries", "dataset"],
            "keywords": {
                "fr": ["données", "Données ouvertes"],
                "en": ["data"],
                "de": ["data"]
            },
            "name": {
